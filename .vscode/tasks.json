{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Tslint",
            "type": "shell",
            "command": "${workspaceFolder}/scripts/lint.sh",
            "args": [
                "typescript"
            ],
            "group": "test",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "dedicated",
                "showReuseMessage": false
            }
        },
        {
            "type": "npm",
            "script": "watch",
            "path": "vinoteca/",
            "group": "build",
            "isBackground": true,
            "problemMatcher": [
                "$ts-webpack-watch",
                "$tslint-webpack-watch"
            ]
        },
        {
            "type": "npm",
            "script": "install",
            "path": "vinoteca/",
            "problemMatcher": []
        },
        {
            "type": "npm",
            "script": "build",
            "path": "vinoteca/",
            "problemMatcher": [
                "$ts-webpack",
                "$tslint-webpack"
            ]
        },
        {
            "type": "npm",
            "script": "profile",
            "path": "vinoteca/",
            "problemMatcher": []
        },
        {
            "type": "npm",
            "script": "analyze-bundle",
            "path": "vinoteca/",
            "problemMatcher": []
        },
        {
            "label": "cargo: generate contracts",
            "type": "shell",
            "command": "cargo",
            "args": [
                "run",
                "--bin",
                "gen_contracts"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": "$rustc",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "dedicated",
                "showReuseMessage": false
            }
        },
        {
            "type": "cargo",
            "subcommand": "run",
            "problemMatcher": [
                "$rustc"
            ]
        },
        {
            "type": "cargo",
            "subcommand": "build",
            "problemMatcher": [
                "$rustc"
            ],
            "group": "build"
        }
    ],
}
